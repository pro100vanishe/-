data_structure = [
    [1, 2, 3],
    {'a': 4, 'b': 5},
    (6, {'cube': 7, 'drum': 8}),
    "Hello",
    ((), [{(2, 'Urban', ('Urban2', 35))}])
]


def calculate_structure_sum(element):
    total_sum = 0
    total_length = 0

    # Если элемент - это число, добавляем к общей сумме
    if isinstance(element, (int, float)):
        total_sum += element
    # Если элемент - это строка, считаем её длину
    elif isinstance(element, str):
        total_length += len(element)
    # Если элемент - это список, кортеж, множество, обрабатываем его элементы
    elif isinstance(element, (list, tuple, set)):
        for item in element:
            sum_res, len_res = calculate_structure_sum(item)
            total_sum += sum_res
            total_length += len_res
    # Если элемент - это словарь, обрабатываем его значения
    elif isinstance(element, dict):
        for value in element.items():
            sum_res, len_res = calculate_structure_sum(value)
            total_sum += sum_res
            total_length += len_res

    return total_sum, total_length


# Обработка всей структуры данных
total_sum, total_length = calculate_structure_sum(data_structure)
print(f"Сумма: {total_sum + total_length }")

